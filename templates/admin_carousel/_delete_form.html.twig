<form method="post" action="{{ path('app_admin_carousel_delete', {'id': carousel.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ carousel.id) }}">
<button class="btn btn-danger btn-sm"><i class="align-middle" data-feather="trash-2"></i></button>
</form>
